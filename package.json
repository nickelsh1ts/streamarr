{
  "name": "streamarr",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nodemon -e ts --watch server --watch streamarr-api.yml -e .json,.ts,.yml -x ts-node -r tsconfig-paths/register --files --project server/tsconfig.json server/index.ts",
    "build:next": "next build",
    "build:server": "tsc --project server/tsconfig.json && copyfiles -u 2 server/templates/**/*.{html,pug} dist/templates && tsc-alias -p server/tsconfig.json",
    "build": "yarn build:next && yarn build:server",
    "prepare": "yarn format && yarn lint && yarn css-lint && yarn typecheck",
    "lint": "eslint \"./server/**/*.{ts,tsx}\" \"./src/**/*.{ts,tsx}\" --cache",
    "start": "NODE_ENV=production node dist/index.js",
    "css-lint": "stylelint src/styles/*.css public/*.css",
    "i18n:extract": "formatjs extract 'src/**/*.{ts,tsx}' --ignore='src/**/*.d.ts' --out-file src/i18n/locale/en.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "migration:generate": "ts-node -r tsconfig-paths/register --project server/tsconfig.json ./node_modules/typeorm/cli.js migration:generate -d server/datasource.ts",
    "migration:create": "ts-node -r tsconfig-paths/register --project server/tsconfig.json ./node_modules/typeorm/cli.js migration:create",
    "migration:run": "ts-node -r tsconfig-paths/register --project server/tsconfig.json ./node_modules/typeorm/cli.js migration:run -d server/datasource.ts",
    "format": "prettier 'src/**/*.{css,js,ts,tsx}' 'server/**/*.{css,js,ts,tsx}' --log-level warn --write --cache .",
    "format:check": "prettier 'src/**/*.{css,js,ts,tsx}' 'server/**/*.{css,js,ts,tsx}' --check --cache .",
    "typecheck": "yarn typecheck:server && yarn typecheck:client",
    "typecheck:server": "tsc --project server/tsconfig.json --noEmit",
    "typecheck:client": "tsc --noEmit",
    "cypress:open": "cypress open",
    "cypress:prepare": "ts-node -r tsconfig-paths/register --files --project server/tsconfig.json server/scripts/prepareTestDb.ts",
    "cypress:build": "yarn build && yarn cypress:prepare",
    "css-build": "tailwindcss -o public/tailwind.css",
    "css-watch": "tailwindcss -o public/tailwind.css --watch",
    "start:python": "cd server/python && gunicorn -w 2 -b 0.0.0.0:5005 --timeout 60 --graceful-timeout 30 --keep-alive 5 plex_invite:app",
    "dev:all": "concurrently \"yarn dev\" \"cd server/python && python3 plex_invite.py\"",
    "start:all": "concurrently \"yarn start\" \"yarn start:python\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nickelsh1ts/streamarr.git"
  },
  "license": "MIT",
  "dependencies": {
    "@headlessui/react": "^2.2.4",
    "@heroicons/react": "^2.2.0",
    "axios": "^1.10.0",
    "axios-rate-limit": "^1.4.0",
    "bcrypt": "^6.0.0",
    "bowser": "^2.11.0",
    "connect-typeorm": "^2.0.0",
    "cookie-parser": "^1.4.7",
    "cronstrue": "^2.61.0",
    "csurf": "^1.11.0",
    "date-fns": "^4.1.0",
    "email-templates": "^12.0.3",
    "express": "^4.21.2",
    "express-openapi-validator": "^5.5.7",
    "express-rate-limit": "^7.5.0",
    "formik": "^2.4.6",
    "ical": "^0.8.0",
    "jose": "^6.0.11",
    "moment": "^2.30.1",
    "multer": "^2.0.2",
    "next": "^15.3.4",
    "nextjs-toploader": "^3.8.16",
    "node-cache": "^5.1.2",
    "node-gyp": "^11.2.0",
    "node-schedule": "^2.1.1",
    "nodemailer": "^7.0.3",
    "openpgp": "^6.1.1",
    "plex-api": "^5.3.2",
    "qrcode": "^1.5.4",
    "react": "^19.1.0",
    "react-animate-height": "^3.2.3",
    "react-aria": "3.41.1",
    "react-big-calendar": "^1.19.4",
    "react-datepicker": "^8.4.0",
    "react-dom": "19.1.0",
    "react-hot-toast": "^2.5.2",
    "react-intl": "7.1.11",
    "react-markdown": "^10.1.0",
    "react-popper-tooltip": "^4.4.2",
    "react-select": "^5.10.1",
    "react-spring": "^10.0.1",
    "react-use-clipboard": "1.0.9",
    "reflect-metadata": "^0.2.2",
    "remark-gfm": "^4.0.1",
    "secure-random-password": "^0.2.3",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "sqlite3": "^5.1.7",
    "swagger-ui-express": "^5.0.1",
    "swr": "^2.3.3",
    "theme-change": "^2.5.0",
    "typeorm": "^0.3.25",
    "ua-parser-js": "^2.0.4",
    "usehooks-ts": "^3.1.1",
    "web-push": "^3.6.7",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "xml2js": "^0.6.2",
    "yamljs": "^0.3.0",
    "yup": "1.6.1"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@eslint/compat": "^1.3.0",
    "@eslint/js": "^9.29.0",
    "@formatjs/cli": "^6.7.2",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "13.0.1",
    "@semantic-release/exec": "7.1.0",
    "@semantic-release/git": "10.0.1",
    "@svgr/webpack": "8.1.0",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "0.5.10",
    "@tailwindcss/typography": "0.5.16",
    "@types/cookie-parser": "^1.4.9",
    "@types/copyfiles": "^2.4.4",
    "@types/csurf": "^1.11.5",
    "@types/email-templates": "^10.0.4",
    "@types/express": "^5.0.3",
    "@types/ical": "^0.8.3",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.0.3",
    "@types/node-schedule": "^2.1.7",
    "@types/nodemailer": "^6.4.17",
    "@types/qrcode": "^1.5.5",
    "@types/react": "19.1.8",
    "@types/react-big-calendar": "^1.16.2",
    "@types/react-dom": "19.1.6",
    "@types/react-transition-group": "4.4.12",
    "@types/secure-random-password": "^0.2.3",
    "@types/semver": "7.7.0",
    "@types/web-push": "^3.6.4",
    "@types/xml2js": "^0.4.14",
    "@types/yamljs": "^0.2.34",
    "@typescript-eslint/eslint-plugin": "8.34.1",
    "@typescript-eslint/parser": "8.34.1",
    "autoprefixer": "10.4.21",
    "commitizen": "4.3.1",
    "concurrently": "^9.2.0",
    "copyfiles": "^2.4.1",
    "crypto": "^1.0.1",
    "cy-mobile-commands": "^0.3.0",
    "cypress": "^14.5.0",
    "cz-conventional-changelog": "3.3.0",
    "daisyui": "4.12.24",
    "eslint": "^9.29.0",
    "eslint-config-next": "^15.3.4",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-formatjs": "5.4.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-no-relative-import-paths": "1.6.1",
    "eslint-plugin-prettier": "5.5.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "globals": "^16.2.0",
    "lint-staged": "16.1.2",
    "nodemon": "^3.1.10",
    "postcss": "8.5.6",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "4.1.0",
    "prettier-plugin-tailwindcss": "0.6.13",
    "sass": "^1.89.2",
    "semantic-release": "24.2.5",
    "stylelint": "^16.21.0",
    "tailwindcss": "3.4.17",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.8.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx,js}": [
      "prettier --write",
      "eslint"
    ],
    "**/*.{json,md,css}": [
      "prettier --write"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "CHANGELOG.md"
          ],
          "message": "chore(release): ${nextRelease.version}"
        }
      ]
    ],
    "branches": [
      "main"
    ],
    "npmPublish": false,
    "publish": [
      {
        "buildArgs": {
          "COMMIT_TAG": "$GIT_SHA"
        },
        "imageNames": [
          "nickelsh1ts/streamarr",
          "ghcr.io/nickelsh1ts/streamarr"
        ],
        "platforms": [
          "linux/amd64",
          "linux/arm64"
        ]
      },
      "@semantic-release/github"
    ]
  },
  "packageManager": "yarn@4.9.2"
}
